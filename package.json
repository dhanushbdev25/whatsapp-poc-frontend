{
  "name": "sample-skeleton",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "cross-env ANALYZE=true vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "lint-fix": "eslint --fix src/**/*.{ts,tsx}",
    "pretty": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,html}\"",
    "pretty:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,html}\"",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "pre-push": "npx tsc --noEmit && npm run lint && npm run pretty:check && npm run build",
    "check-deps": "npm outdated",
    "check-security": "npm audit --audit-level=moderate",
    "check-all": "npm run lint && npm run pretty:check && npx tsc --noEmit && npm run build && npm run check-deps && npm run check-security",
    "setup-hooks": "node scripts/setup-hooks.js",
    "test-hooks": "npm run setup-hooks"
  },
  "dependencies": {
    "@ant-design/colors": "^7.2.1",
    "@azure/msal-browser": "^4.24.1",
    "@azure/msal-react": "^3.0.19",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/material-pigment-css": "^7.3.3",
    "@mui/x-date-pickers": "^8.12.0",
    "@reduxjs/toolkit": "^2.9.0",
    "@stripe/react-stripe-js": "^5.3.0",
    "@stripe/stripe-js": "^8.3.0",
    "@tensorflow-models/body-pix": "^2.2.1",
    "@tensorflow/tfjs": "^4.22.0",
    "@zxing/browser": "^0.1.5",
    "axios": "^1.13.2",
    "dayjs": "^1.11.10",
    "konva": "^10.0.2",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.552.0",
    "material-react-table": "^3.2.1",
    "mui": "^0.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.64.0",
    "react-icons": "^5.5.0",
    "react-konva": "^19.0.10",
    "react-loader-spinner": "^8.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.4",
    "react-webcam": "^7.2.0",
    "recharts": "^3.3.0",
    "sweetalert2": "^11.26.3",
    "vite-plugin-environment": "^1.1.3",
    "yup": "^1.3.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.37.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.4",
    "terser": "^5.44.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.9"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix --max-warnings 0",
      "prettier --write"
    ],
    "src/**/*.{js,jsx}": [
      "prettier --write"
    ],
    "src/**/*.{css,scss}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "happy-dom": "^20.0.0",
    "@wyw-in-js/transform": "^0.7.0"
  }
}
